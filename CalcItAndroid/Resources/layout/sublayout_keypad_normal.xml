<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:stretchColumns="*">
	<TableRow
		android:layout_width="match_parent"
		android:layout_height="fill_parent"
		android:layout_weight="1">
		<Button
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:text="sin("
			android:id="@+id/calculatorButton111"
			style="@style/CalculatorButton"/>
		<Button
			android:text="cos("
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:id="@+id/calculatorButton112"
			style="@style/CalculatorButton"/>
		<Button
			android:text="tan("
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:id="@+id/calculatorButton113"
			style="@style/CalculatorButton"/>
		<Button
			android:text="log("
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:id="@+id/calculatorButton114"
			style="@style/CalculatorButton"/>
		<Button
			android:text="KBD"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:id="@+id/calculatorButton115"
			style="@style/CalculatorButton"/>
		<Button
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:text="SHIFT"
			android:id="@+id/calculatorButton116"
			style="@style/CalculatorButton"/>
	</TableRow>
	<TableRow
		android:layout_width="match_parent"
		android:layout_height="fill_parent"
		android:layout_weight="1">
		<Button
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:text="-"
			android:id="@+id/calculatorButton121"
			style="@style/CalculatorButton"/>
		<Button
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:text="("
			android:id="@+id/calculatorButton122"
			style="@style/CalculatorButton"/>
		<Button
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:text=")"
			android:id="@+id/calculatorButton123"
			style="@style/CalculatorButton"/>
		<Button
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:text=";"
			android:id="@+id/calculatorButton124"
			style="@style/CalculatorButton"/>
		<Button
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:text="^"
			android:id="@+id/calculatorButton125"
			style="@style/CalculatorButton"/>
		<Button
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:text="#"
			android:id="@+id/calculatorButton126"
			style="@style/CalculatorButton"/>
	</TableRow>
	<TableRow
		android:layout_width="match_parent"
		android:layout_height="fill_parent"
		android:layout_weight="4">
		<TableLayout
			android:orientation="vertical"
			android:layout_width="0dp"
			android:layout_weight="2"
			android:layout_height="match_parent"
			android:stretchColumns="*">
			<TableRow
				android:layout_width="match_parent"
				android:layout_weight="1"
				android:layout_height="fill_parent">
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="7"
					android:id="@+id/calculatorButton131"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="8"
					android:id="@+id/calculatorButton132"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="9"
					android:id="@+id/calculatorButton133"
					style="@style/CalculatorButton"/>
			</TableRow>
			<TableRow
				android:layout_width="match_parent"
				android:layout_weight="1"
				android:layout_height="fill_parent">
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="4"
					android:id="@+id/calculatorButton141"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="5"
					android:id="@+id/calculatorButton142"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="6"
					android:id="@+id/calculatorButton143"
					style="@style/CalculatorButton"/>
			</TableRow>
			<TableRow
				android:layout_width="match_parent"
				android:layout_weight="1"
				android:layout_height="fill_parent">
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="1"
					android:id="@+id/calculatorButton151"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="2"
					android:id="@+id/calculatorButton152"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="3"
					android:id="@+id/calculatorButton153"
					style="@style/CalculatorButton"/>
			</TableRow>
			<TableRow
				android:layout_width="match_parent"
				android:layout_weight="1"
				android:layout_height="fill_parent">
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="0"
					android:id="@+id/calculatorButton161"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text=","
					android:id="@+id/calculatorButton162"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text=".10^"
					android:id="@+id/calculatorButton163"
					style="@style/CalculatorButton"/>
			</TableRow>
		</TableLayout>
		<TableLayout
			android:orientation="vertical"
			android:layout_width="0dp"
			android:layout_weight="1"
			android:layout_height="match_parent"
			android:stretchColumns="*">
			<TableRow
				android:layout_width="match_parent"
				android:layout_weight="1"
				android:layout_height="fill_parent">
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="AC"
					android:id="@+id/calculatorButton134"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="DEL"
					android:id="@+id/calculatorButton135"
					style="@style/CalculatorButton"/>
			</TableRow>
			<TableRow
				android:layout_width="match_parent"
				android:layout_weight="1"
				android:layout_height="fill_parent">
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="."
					android:id="@+id/calculatorButton144"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text=":"
					android:id="@+id/calculatorButton145"
					style="@style/CalculatorButton"/>
			</TableRow>
			<TableRow
				android:layout_width="match_parent"
				android:layout_weight="1"
				android:layout_height="fill_parent">
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="+"
					android:id="@+id/calculatorButton154"
					style="@style/CalculatorButton"/>
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="â€“"
					android:id="@+id/calculatorButton155"
					style="@style/CalculatorButton"/>
			</TableRow>
			<TableRow
				android:layout_width="match_parent"
				android:layout_weight="1"
				android:layout_height="fill_parent">
				<Button
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:text="Calculate"
					android:id="@+id/calculatorButtonCalculate"
					style="@style/CalculatorButton"/>
			</TableRow>
		</TableLayout>
	</TableRow>
</TableLayout>
